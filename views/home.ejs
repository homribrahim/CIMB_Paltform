			</div>

		</div>
	</div>
	</div><!-- End Sidebar-->


	<div class="sidebar-overlay"></div>

	<!--Content Start-->

	<!--Pop Up ALERT-->

	<div class="popup_box">
		<i class="fas fa-exclamation" style=" font-size: 60px;
		color: #081A51;
		border: 5px solid #081A51;
		padding: 20px 40px;
		border-radius: 50%;
		margin: -10px 0 20px 0;"></i>
		<h1>Etes-Vous Encore Là ? <i class="fas fa-grin-beam-sweat fa-1x" style="color: #081A51;"></i> </h1>
		<label>Anis Balti</label>
		<div class="btns">
			<a href="#" class="btn1">&nbsp;&nbsp;&nbsp; Oui &nbsp;&nbsp;&nbsp;</a>
		</div>
	</div>
	<!--End Pop Up ALERT-->

	<div class="content-start transition  ">
		<div class="container-fluid dashboard">
			<div class="content-header">
				<form class="form-inline col-sm-12 col-lg-6 col-md-12 mt-3" action="/users/home" method="POST">
					<label for="search">Chercher Les Actualités</label>
					<input class="form-control mr-sm-2" name="search" type="search" aria-label=" Search">
					<button type="submit" class="btn btn-primary mt-3">Chercher</button>
					<button type="reset" class="btn btn-outline-secondary mt-3">Annuler</button>
				</form>
				<label for="MyFormuleDisplay">En Millimes</label>
				<div id="MyFormuleDisplay" class="formule" onload="formCalc()" style="color:#081A51 ;"></div>
				<label for="MyPointsDisplay">Point/Minute</label>
				<div id="MyPointsDisplay" class="points" onload="pointCalc()" style="color:#081A51 ;"></div>
				<div class="news-container ">
					<div class="news">
						<% art.forEach(function(article,index){ %>
							<% if ((typeof article.url=='object' ) || (typeof article.title=='object' ) || (typeof
								article.urlToImage=='object' ) || (typeof article.content=='object' )){ %>
								<% } else{ %>
									<a href="<%- article.url %>" target="_blank" class="news-box Hover-effect">

										<img src="<%- article.urlToImage %>" alt="Image">
										<h3 style="margin-top: 30px;">
											<%- article.title %>
										</h3>
										<p
											style="position: relative; width: 100%; text-align: center; color: white; margin-top: 40px;">
											<%- article.content.replace(/<[^>]+>/g, '') %>
										</p>
										<br>
										<p style="position:relative; margin-bottom:5px">
											Updated: <span style="color:red;">
												<%- new Date(article.publishedAt.slice(0,10)).toDateString() %>
													<%- moment.utc(article.publishedAt).local().format('hh : mm A') %>
											</span></p>
									</a>
									<% } %>
										<% }) %>
					</div>
				</div>
			</div>

		</div><!-- End Container-->
	</div><!-- End Content-->

	<!-- Footer -->
	<footer>
		<div class="footer">
			<div class="float-start">
				<p>2022 &copy; CIMB</p>
			</div>
			<div class="float-end">
				<p>Crafted with
					<span class="text-danger">
						<i class="fa fa-heart"></i> by
						<a href="https://www.facebook.com/andreew.co.id/" class="author-footer">&nbsp;
							Brahim & Yassin
						</a>
					</span>
				</p>
			</div>
		</div>
	</footer>


	<!-- Preloader -->
	<div class="loader">
		<div class="spinner-border text-light" role="status">
			<span class="sr-only">Loading...</span>
		</div>
	</div>

	<!-- Loader -->
	<div class="loader-overlay"></div>




	<!-- Display Points -->
	<script>
		var points = 0;
		function pointCalc() {
			points += 1;
			document.getElementById("MyPointsDisplay").innerText = points;
			document.getElementById("MyPointsDisplay").textContent = points;
			setTimeout(pointCalc, 10000);
			return points;
		}

		pointCalc();
	</script>

	<!-- Display Formule -->
	<script>
		var formule = 0;
		function formCalc() {
			formule += 0.45;
			document.getElementById("MyFormuleDisplay").innerText = formule.toFixed(2);
			document.getElementById("MyFormuleDisplay").textContent = formule.toFixed(2);
			setTimeout(formCalc, 1000);
			return formule;
		}

		formCalc();
	</script>

	<script>
		async function stopCalc() {
			alert("Vous Avez " + formCalc().toFixed(2) + " Millimes Avec " + pointCalc() + "Points");
		}
	</script>

	<script>

		$(document).ready(function () {
			setTimeout(function () {
				$('.popup_box').css("display", "block");
				$('.btn1').click(function () {
					$('.popup_box').css("display", "none");
				});
			}, 10000);
		});

	</script>

	<!-- Chrono Script -->

	<script>
		function showTime() {
			var date = new Date();
			var h = date.getHours(); // 0 - 23
			var m = date.getMinutes(); // 0 - 59
			var s = date.getSeconds(); // 0 - 59
			var d = date.getDay(); // 0 - 30
			switch (new Date().getDay()) {
				case 0:
					d = "Dimanche";
					break;
				case 1:
					d = "Lundi";
					break;
				case 2:
					d = "Mardi";
					break;
				case 3:
					d = "Mercredi";
					break;
				case 4:
					d = "Jeudi";
					break;
				case 5:
					d = "Vendredi";
					break;
				case 6:
					d = "Samedi";
			}
			var time = d + " , " + h + ":" + ((m < 10) ? '0' + m : + m) + ":" + ((s < 10) ? '0' + s : + s) + " ";
			document.getElementById("MyClockDisplay").innerText = time;
			document.getElementById("MyClockDisplay").textContent = time;
			setTimeout(showTime, 1000);
		}
		showTime();
	</script>

	<!-- API SCRIPT -->

	<script src="moment.js"></script>
	<script src="moment-timezone-with-data.js"></script>
